  post:
    description: Create a product-images
    body:
      application/json:
        type: object
        properties:
          product:
            type: integer
          discount_percentage:
            type: integer
          start_date:
            type: string
          end_date:
            type: string

    responses:
      201:
        body:
          application/json:
            example: |
                {
                  "id": 2,
                  "product": 10,
                  "discount_percentage": 20.0,
                  "start_date": "2024-10-13",
