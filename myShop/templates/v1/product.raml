        204:

description: 3.Category Management (CRUD)
/categories/:
  get:
    description: List all categories
    queryParameters:
      id:
        description: unique identifier of a category in database
        type: integer     
      name:
        description:  name of a specific category that a product belongs to in a database
        type: string  
    responses:
      200:
        body:
          application/json:
            example: | 
              [
                {
                  "id": 1,
                  "name": "Home decor"
                },
                {
                  "id": 2,
                  "name": "Electronics"
                },
                {
                  "id": 3,
                  "name": "Men's fashion"
                },
                {
                  "id": 4,
                  "name": "Women's fashion"
                }
              ]
              
  post:
    description: Create a category
    body:
      application/json:
        type: object
        properties:
          name:
            type: string

    responses:
      201:
        body:
          application/json:
            example: |
              {
                "id": 7,
                "name": "Nouriture"
              }

  /categories{id}:
    get:
      description: Retrieve a single category by ID
      responses:
        200:
          body:
            application/json:
              example: |
                {
                  "id": 1,
                  "name": "Home decor"
                }
                  
    put:
      description: Update a category details
      body:
        application/json:
          type: object
          properties:
            id:
              type: integer
            name:
              type: string
            
      responses:
        200:
          body:
            application/json:
              example: |
                {
                  "id": 7,
                  "name": "Equipment"
                }
                 
    delete:
      description: Delete a category by ID
      responses:
        204:

