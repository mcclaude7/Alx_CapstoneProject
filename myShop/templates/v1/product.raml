      id:
        description: unique identifier of a promotion in database
        type: integer     
      product:
         description: unique identifier of a product in database 
         type: integer
      discount_percentage:
        description: original price of a product  that is reduced during a sale or promotion
        type: number
      start_date:
        description: specific day on which a promotion is scheduled to begin.
        type: string
      End_date:
        description: specific day on which a promotion is scheduled to end.
        type: string
        
      
    responses:
      200:
        body:
