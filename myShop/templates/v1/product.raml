description: 2.Review Management (CRUD)
/reviews/:
  get:
    description: List all products
    queryParameters:
      id:
        description: unique identifier of a review in database
        type: integer     
      User_id:
        description: unique identifier assigned to a user in a database
        type: integer
      product_id:  
        description: unique identifier assigned to a product in a database 
        type: integer    
      rating:
        description: Represent user rating for a products(number between 1 and 5)
        type: integer
      comment:
        description: user's comment or review for a product
        type: text
      created_at:
        description: the image address of a product 
        type: string
      stock_quantity:
        description: the number of unit of a product
        type: number
      Review:
        description: feedback provided by a user or customer about a product
        type: dictionary
      images:
        description: Images related to the corresponding product
        type: string
      ctreated_date:
        description: represents the timestamps indicating when a particular record was created in database
        type: string
    responses:
      200:
        body:
          application/json:
            example: |
              [
                {
                  "id": 1,
                  "user": 1,
                  "product": 1,
                  "rating": 4,
                  "comment": "Nziza cyane",
                  "created_at": "2024-10-12T12:51:39.966398Z"
                },
                {
                  "id": 2,
                  "user": 3,
                  "product": 4,
                  "rating": 5,
                  "comment": "narayiguze narayiguzw narayiguzw a arayiguze narayiguzw narayiguzwrayiguze narayiguzw narayiguzw arayiguze narayiguzw narayiguzw arayiguze narayiguzw narayiguzw arayiguze narayiguzw narayiguzw",
                  "created_at": "2024-10-12T13:01:56.567341Z"
                }
              ]

  post:
    description: Create a review
    body:
      application/json:
        type: object
        properties:
          product:
            type: integer
          rating:
            type: integer
          comment:
            type: text

    responses:
      201:
        body:
          application/json:
            example: |
              {
                "id": 1,
                "user": 1,
                "product": 1,
                "rating": 4,
                "comment": "Nziza cyane",
                "created_at": "2024-10-12T12:51:39.966398Z"
              }

  /reviews/{id}:
    get:
      description: Retrieve a single review by ID
      responses:
        200:
          body:
            application/json:
              example: |

                {
                  "id": 2,
                  "user": 3,
                  "product": 4,
                  "rating": 5,
                  "comment": "narayiguze narayiguzw narayiguzw a arayiguze narayiguzw narayiguzwrayiguze narayiguzw narayiguzw arayiguze narayiguzw narayiguzw arayiguze narayiguzw narayiguzw arayiguze narayiguzw narayiguzw",
                  "created_at": "2024-10-12T13:01:56.567341Z"
                }

    put:
      description: Update a review details
      body:
        application/json:
          type: object
          properties:
            name:
              type: string
            description:
              type: string
            price:
              type: integer
            category:
              type: string
            stock_quantity:
              type: integer
            image_url:
              type: string
      responses:
        200:
          body:
            application/json:
              example: |
                {
                  "id": 2,
                  "user": 3,
                  "product": 4,
                  "rating": 5,
                  "comment": "good item",
                  "created_at": "2024-10-12T13:01:56.567341Z"
