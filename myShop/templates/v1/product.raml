    body:
      application/json:
        type: object
        properties:
          quantity:
            type: integer
    responses:
      201:
        body:
          application/json:
            example: |
              {
                "message": "You have successfully purchased 3 of Samsung Galaxy M55s 5G (Coral Green,8GB RAM,128GB Storage).",
                "remaining_stock": 4,
                "product_details": 
                  {
                    "id": 4,
                    "name": "Samsung Galaxy M55s 5G (Coral Green,8GB RAM,128GB Storage)",
                    "description": "DISPLAY - 16.95 Centimeters (6.7\"Inch) Super AMOLED Plus Display with 19.5:9 Aspect Ratio, FHD+ Resolution with 1080 x 2400 Pixels , 394 PPI with 16M Colors and 120Hz Refresh Rate",
                    "price": "345000.00",
                    "category": 
                      {
                        "id": 5,
                        "name": "Mobiles, Laptops"
                      },
                    "image_url": "https://m.media-amazon.com/images/I/41iXRIjYK5L._SX300_SY300_QL70_FMwebp_.jpg",
                    "stock_quantity": 4,
                    "reviews": [],
                    "images": 
                      [
                        {
                          "id": 5,
                          "product": 4,
